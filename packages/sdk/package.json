{
  "name": "@embed-ai/sdk",
  "version": "0.0.0",
  "type": "module",
  "license": "BSD-3-Clause",
  "description": "The typescript sdk package for embed AI APIs.",
  "repository": {
    "type": "git",
    "url": "https://github.com/ZKAI-Network/embed-sdk/",
    "directory": "packages/sdk"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "scripts": {
    "codegen": "build-utils prepare-v2",
    "build": "bun run build-esm && bun run build-annotate && bun run build-cjs && build-utils pack-v2 && bun run fix-deps",
    "build-esm": "tsc --project tsconfig.build.json --noEmit && tsc --project tsconfig.src.json",
    "build-cjs": "babel build/esm --plugins @babel/transform-export-namespace-from --plugins @babel/transform-modules-commonjs --out-dir build/cjs --source-maps",
    "build-annotate": "babel build/esm --plugins annotate-pure-calls --out-dir build/esm --source-maps",
    "fix-deps": "bun scripts/fix-deps.js",
    "check": "tsc -b tsconfig.json",
    "test": "vitest",
    "coverage": "vitest --coverage"
  },
  "dependencies": {
    "@embed-ai/types": "workspace:*",
    "effect": "latest"
  },
  "effect": {
    "generateExports": {
      "include": [
        "**/*.ts"
      ]
    },
    "generateIndex": {
      "include": [
        "**/*.ts"
      ]
    }
  }
}
